# CoreTet V2 Environment Variables
# Copy this to .env.local for V2 development

# Supabase (same as V1)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# V2 Feature Flags
VITE_ENABLE_V2=true
VITE_ENABLE_COLLAB=true
VITE_ENABLE_PRESENCE=false  # Enable when ready
VITE_ENABLE_AI=false         # Enable when ready
VITE_ENABLE_SMART_SEARCH=false
VITE_ENABLE_AUTO_TAG=false
VITE_ENABLE_GRAPH=false
VITE_SHOW_MIGRATION=false   # Show migration prompt to users

# Version Info
VITE_APP_VERSION=2.0.0-beta
VITE_APP_ENV=development

# API Configuration
VITE_API_SCHEMA=public      # Change to 'v2' when using separate schema

# Analytics (optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=