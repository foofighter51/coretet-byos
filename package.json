{
  "name": "coretet_no_ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:v1": "VITE_ENABLE_V2=false vite",
    "dev:v2": "VITE_ENABLE_V2=true vite",
    "build": "node scripts/update-version.js && vite build",
    "build:v1": "VITE_ENABLE_V2=false npm run build",
    "build:v2": "VITE_ENABLE_V2=true npm run build",
    "build:only": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "create-test-user": "node scripts/createTestUser.js",
    "check-database": "node scripts/check-database.js",
    "check-data": "node scripts/check-data.js",
    "check-schema": "node scripts/check-schema.js",
    "setup-v2-db": "node scripts/setup-v2-database.js",
    "v1": "node scripts/switch-version.js v1",
    "v2": "node scripts/switch-version.js v2",
    "deploy:beta": "npm run build:v2 && netlify deploy --prod --site=coretet-beta",
    "deploy:prod": "npm run build:v1 && netlify deploy --prod"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.52.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.7.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
